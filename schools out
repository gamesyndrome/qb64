    IF MSTR3X >= CROX - 30 AND MSTR3X <= CROX + 30 THEN
        IF MSTR3Y >= CROY - 30 AND MSTR3X <= CROY + 30 THEN
            MSTR3ON = 0
            PLSCO = PLSCO + 50
            _SNDPLAYFILE "holy.ogg"
        END IF
    END IF
    IF MSTR4X >= CROX - 30 AND MSTR4X <= CROX + 30 THEN
        IF MSTR4Y >= CROY - 30 AND MSTR4X <= CROY + 30 THEN
            MSTR4ON = 0
            PLSCO = PLSCO + 50
            _SNDPLAYFILE "holy.ogg"
        END IF
    END IF
    IF MSTR5X >= CROX - 30 AND MSTR5X <= CROX + 30 THEN
        IF MSTR5Y >= CROY - 30 AND MSTR5X <= CROY + 30 THEN
            MSTR5ON = 0
            PLSCO = PLSCO + 50
            _SNDPLAYFILE "holy.ogg"
        END IF
    END IF
    IF MSTR6X >= CROX - 30 AND MSTR6X <= CROX + 30 THEN
        IF MSTR6Y >= CROY - 30 AND MSTR6X <= CROY + 30 THEN
            MSTR6ON = 0
            PLSCO = PLSCO + 50
            _SNDPLAYFILE "holy.ogg"
        END IF
    END IF
    CROSSNTTIM = CROSSNTTIM + 1
    IF CROSSNTTIM = 2000 THEN
        CROSSNT = 0
        CROSSNTTIM = 0
    END IF




END IF


IF PLDER = 1 THEN
    LINE (PLX, PLY + 1)-(PLX, PLY + 6), SHTCLR
    LINE (PLX + 1, PLY)-(PLX + 1, PLY + 7), SHTCLR
    LINE (PLX + 2, PLY)-(PLX + 2, PLY + 7), SHTCLR
    LINE (PLX + 3, PLY + 1)-(PLX + 3, PLY + 6), SHTCLR

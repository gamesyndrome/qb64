    END IF
    IF MSTR1X = PLX AND MSTR1Y = PLY THEN
        _SNDPLAYFILE "hit.ogg"
        RANDOMIZE TIMER
        LT = INT(RND * 2)
        IF LT = 0 THEN LIFE = LIFE - 1
        IF LT = 1 THEN LIFE = LIFE - 2
        MSTR1ON = 0
        IF LIFE <= 0 THEN GOTO 20000
    END IF
    LINE (MSTR2X + 1, MSTR2Y)-(MSTR2X + 1, MSTR2Y + 6), 0

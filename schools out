    LINE (MSTR1X + 5, MSTR1Y)-(MSTR1X + 5, MSTR1Y + 6), 0
    LINE (MSTR1X, MSTR1Y + 2)-(MSTR1X, MSTR1Y + 4), 0
    LINE (MSTR1X + 2, MSTR1Y + 2)-(MSTR1X + 2, MSTR1Y + 4), 0
    LINE (MSTR1X + 4, MSTR1Y + 2)-(MSTR1X + 4, MSTR1Y + 4), 0
    LINE (MSTR1X + 6, MSTR1Y + 2)-(MSTR1X + 6, MSTR1Y + 4), 0
    IF PLBUX >= MSTR1X AND PLBUX <= MSTR1X + 6 THEN
        IF PLBUY >= MSTR1Y AND PLBUY <= MSTR1Y + 6 THEN
            MSTR1DK = MSTR1DK + 1


PRINT SCO7TH$; ":"; SCO7TH
PRINT SCO8TH$; ":"; SCO8TH
PRINT "======================================"
PRINT "Type Q to end game"
PLSCO = 0
INPUT DUM$
IF DUM$ = "Q" THEN SYSTEM
_SNDSTOP go&
LVL = 0
GOTO 10
25000
_SNDSTOP bg&
_SNDPLAY win&
CLS
IF LVL = 8 GOTO 30000
PRINT "You made it home!! Going to next level! :)"
PRINT "TIME EARN BONUS:"; EARN
PLSCO = PLSCO + EARN
IF LIFE = 30 THEN
    PRINT "PERFECT!!: 157500"
    PLSCO = PLSCO + 157500
END IF
PRINT "SCORE:"; PLSCO
INPUT DUM$
_SNDSTOP win&
GOTO 10
30000
_SNDLOOP te&
COLOR 15, 11
CLS
PRINT "The demons are gone!! The town is saved! :)"


IF MSTR5ON = 0 THEN MSTRSPDC = 0
IF MSTR6ON = 0 THEN MSTRSPDC = 0


IF SPDPUT = 1 THEN SPDTIM = SPDTIM + 1
IF SPDTIM = 10 THEN SPDTIM = 0: SPDPUT = 0

IF SPDPUT = 1 THEN
    IF PLDER = 1 THEN PLX = PLX - 1
    IF PLDER = 2 THEN PLX = PLX + 1
    IF PLDER = 3 THEN PLY = PLY - 1
    IF PLDER = 4 THEN PLY = PLY + 1
END IF

IF SPDBOTPLC = 1 THEN
    IF SCRR = 9 THEN
        IF LVL = 7 THEN
            LINE (100, 100)-(102, 100), 1
            LINE (100, 100)-(102, 101), 1
            LINE (100, 102)-(102, 102), 1
            LINE (100, 103)-(105, 103), 1
            LINE (100, 104)-(105, 104), 1
            LINE (100, 105)-(106, 105), 1
        END IF
    END IF
END IF
IF SPDBOTPLC = 1 AND SCRR = 9 THEN
    IF PLX >= 100 AND PLX <= 105 THEN
        IF PLX > 100 AND PLY <= 106 THEN
            IF PLSCO >= 200000 AND LVL = 7 THEN
                PLSCO = PLSCO - 200000
                SPDBOTPLC = 0
                SPDBOT = 1
            END IF
        END IF
    END IF
END IF
IF GUNPLC = 1 THEN
    IF SCRR = 8 THEN
        LINE (180, 140)-(186, 140), 0
        LINE (186, 140)-(186, 144), 0
        LINE (184, 140)-(184, 142), 0
        LINE (184, 142)-(186, 142), 0
        IF PLX >= 180 AND PLX <= 186 THEN
            IF PLY >= 140 AND PLY <= 144 THEN
                GUNPLC = 0
                PLWGUN = 1
            END IF
        END IF

    END IF
END IF
IF GERPLC = 1 AND SCRR = 8 THEN
    IF LVL >= 4 THEN
        LINE (180, 180)-(183, 180), 2
        LINE (179, 181)-(184, 181), 2
        LINE (179, 182)-(184, 182), 2
        LINE (180, 183)-(183, 183), 2
        IF PLX >= 179 AND PLX <= 184 THEN
            IF PLY >= 180 AND PLY <= 183 THEN
                GER = 15
                GERPLC = 0
            END IF
        END IF

    END IF
END IF
IF PLX < -14 THEN
    PLX = 350
    SCRR = SCRR - 1
    MSTR1ON = 0
    MSTR2ON = 0
    MSTR3ON = 0
    MSTR4ON = 0
    MSTR5ON = 0
    MSTR6ON = 0
END IF
IF PLX > 354 THEN
    PLX = -12
    SCRR = SCRR + 1
    MSTR1ON = 0
    MSTR2ON = 0
    MSTR3ON = 0
    MSTR4ON = 0
    MSTR5ON = 0
    MSTR6ON = 0
END IF

DO
    I$ = INKEY$
    IF I$ = "w" THEN
        IF SPDBOT = 0 THEN PLDER = 3: PLY = PLY - 1
        IF SPDBOT = 1 THEN PLDER = 3: SPDPUT = 1
    END IF

    IF I$ = "x" THEN
        IF SPDBOT = 0 THEN PLDER = 4: PLY = PLY + 1
        IF SPDBOT = 1 THEN PLDER = 4: SPDPUT = 1
    END IF
    IF I$ = "a" THEN
        IF SPDBOT = 0 THEN PLDER = 1: PLX = PLX - 1
        IF SPDBOT = 1 THEN PLDER = 1: SPDPUT = 1
    END IF
    IF I$ = "d" THEN
        IF SPDBOT = 0 THEN PLDER = 2: PLX = PLX + 1
        IF SPDBOT = 1 THEN PLDER = 2: SPDPUT = 1
    END IF
    IF I$ = "s" THEN
        PLSHT = 1
        IF PLWGUN = 1 THEN _SNDPLAYFILE "gun.ogg"
    END IF
    IF I$ = "f" THEN
        IF GER > 0 THEN
            IF PLDER = 1 THEN GERX = PLX: GERY = PLY + 1
            IF PLDER = 2 THEN GERX = PLX: GERY = PLY + 6
            IF PLDER = 3 THEN GERX = PLX + 1: GERY = PLY
            IF PLDER = 4 THEN GERX = PLX + 6: GERY = PLY
            GER = GER - 1
            GERTHR = 1
            GERF = 1
        END IF
    END IF
    IF I$ = "c" THEN
        IF CROSS > 0 THEN
            IF PLDER = 1 THEN CROX = PLX: CROY = PLY + 1
            IF PLDER = 2 THEN CROX = PLX: CROY = PLY + 6
            IF PLDER = 3 THEN CROX = PLX + 1: CROY = PLY
            IF PLDER = 4 THEN CROX = PLX + 6: CROY = PLY
            CROSS = CROSS - 1
            CROSTHR = 1
        END IF
    END IF
    IF I$ = "e" THEN
        IF SPLBOK = 1 THEN SPLBOKUSE = 1: _SNDPLAYFILE "magic.ogg"
    END IF
    TEM = TEM + 1
    IF TEM = 3000 THEN TEM = 0: GOTO 100
LOOP


20000
_SNDSTOP bg&
_SNDPLAY go&
CLS
PRINT "You have died!! GAME OVER :("

PRINT "SCORE:"; PLSCO
SLEEP
CLS
OPEN "SOSAVEA.DAT" FOR BINARY AS #1
OPEN "SOSAVEB.DAT" FOR INPUT AS #2
INPUT "ENTER YOUR NAME"; SCO$
INPUT #2, SCO1ST$
INPUT #2, SCO2ND$
INPUT #2, SCO3RD$
INPUT #2, SCO4TH$
INPUT #2, SCO5TH$
INPUT #2, SCO6TH$
INPUT #2, SCO7TH$
INPUT #2, SCO8TH$
CLOSE #2
GET #1, 4, SCO1ST
GET #1, 8, SCO2ND
GET #1, 12, SCO3RD
GET #1, 16, SCO4TH
GET #1, 20, SCO5TH
GET #1, 24, SCO6TH
GET #1, 28, SCO7TH
GET #1, 32, SCO8TH
IF PLSCO > SCO1ST THEN
    PRINT "FIRST PLACE!!"


    SCO8TH$ = SCO7TH$
    SCO8TH = SCO7TH
    SCO7TH$ = SCO6TH$
    SCO7TH = SCO6TH
    SCO6TH$ = SCO5TH$
    SCO6TH = SCO5TH
    SCO5TH$ = SCO4TH$
    SCO5TH = SCO4TH
    SCO4TH$ = SCO3RD$
    SCO4TH = SCO3RD
    SCO3RD$ = SCO2ND$
    SCO3RD = SCO2ND
    SCO2ND$ = SCO1ST$
    SCO2ND = SCO1ST

    SCO1ST$ = SCO$
    SCO1ST = PLSCO
    PUT #1, 4, SCO1ST
    PUT #1, 8, SCO2ND
    PUT #1, 12, SCO3RD
    PUT #1, 16, SCO4TH
    PUT #1, 20, SCO5TH
    PUT #1, 24, SCO6TH
    PUT #1, 28, SCO7TH
    PUT #1, 32, SCO8TH



    GOTO 60000
END IF
IF PLSCO > SCO2ND THEN
    PRINT "SECOND PLACE!"
    SCO8TH$ = SCO7TH$
    SCO8TH = SCO7TH
    SCO7TH$ = SCO6TH$
    SCO7TH = SCO6TH
    SCO6TH$ = SCO5TH$
    SCO6TH = SCO5TH
    SCO5TH$ = SCO4TH$
    SCO5TH = SCO4TH
    SCO4TH$ = SCO3RD$
    SCO4TH = SCO3RD
    SCO3RD$ = SCO2ND$
    SCO3RD = SCO2ND

    SCO2ND$ = SCO$
    SCO2ND = PLSCO

    PUT #1, 8, SCO2ND
    PUT #1, 12, SCO3RD
    PUT #1, 16, SCO4TH
    PUT #1, 20, SCO5TH
    PUT #1, 24, SCO6TH
    PUT #1, 28, SCO7TH
    PUT #1, 32, SCO8TH

    GOTO 60000
END IF
IF PLSCO > SCO3RD THEN
    PRINT "THIRD PLACE"
    SCO8TH$ = SCO7TH$
    SCO8TH = SCO7TH
    SCO7TH$ = SCO6TH$
    SCO7TH = SCO6TH
    SCO6TH$ = SCO5TH$
    SCO6TH = SCO5TH
    SCO5TH$ = SCO4TH$
    SCO5TH = SCO4TH
    SCO4TH$ = SCO3RD$
    SCO4TH = SCO3RD


        IF PLBUY >= MSTR2Y AND PLBUY <= MSTR2Y + 6 THEN
            MSTR2DK = MSTR2DK + 1
            PLSHTO = 0
            PLBUX = 800
            PLBUY = 800

            IF MSTR2DK = MSTRK THEN
                _SNDPLAYFILE "demon.ogg"
                PLSCO = PLSCO + 25
                MSTR2DK = 0
                MSTR2ON = 0
            END IF
        END IF
    END IF
    IF MSTR2X = PLX AND MSTR2Y = PLY THEN
        _SNDPLAYFILE "hit.ogg"
        RANDOMIZE TIMER
        LT = INT(RND * 2)
        IF LT = 0 THEN LIFE = LIFE - 1
        IF LT = 1 THEN LIFE = LIFE - 2

        MSTR2ON = 0
        IF LIFE <= 0 THEN GOTO 20000
    END IF

END IF


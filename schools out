MSTRSPCNT = MSTRSPCNT + 1
IF MSTRSPCNT = MSTRSPD THEN
    MSTRSPCNT = 0

    IF MSTRSCR >= 2 THEN
        IF MSTR1X < PLX THEN MSTR1X = MSTR1X + 1
        IF MSTR1X > PLX THEN MSTR1X = MSTR1X - 1
        IF MSTR1Y < PLY THEN MSTR1Y = MSTR1Y + 1
        IF MSTR1Y > PLY THEN MSTR1Y = MSTR1Y - 1

        IF MSTR2X < PLX THEN MSTR2X = MSTR2X + 1
        IF MSTR2X > PLX THEN MSTR2X = MSTR2X - 1
        IF MSTR2Y < PLY THEN MSTR2Y = MSTR2Y + 1
        IF MSTR2Y > PLY THEN MSTR2Y = MSTR2Y - 1

    END IF
    IF MSTRSCR >= 3 THEN
        IF MSTR3X < PLX THEN MSTR3X = MSTR3X + 1
        IF MSTR3X > PLX THEN MSTR3X = MSTR3X - 1
        IF MSTR3Y < PLY THEN MSTR3Y = MSTR3Y + 1
        IF MSTR3Y > PLY THEN MSTR3Y = MSTR3Y - 1

